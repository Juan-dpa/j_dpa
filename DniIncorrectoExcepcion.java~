import java.lang.Exception;

public class DniIncorrectoExcepcion extends Exception{

    public String aux;

    public DniIncorrectoExcepcion(){
	this.aux = "";
    }
    public DniIncorrectoExcepcion(String aux2){
	this.aux = aux2;
    }
}
